 "client_script": "var field = frappe.web_form.get_field('packaging_vendor');\nfield._data = [];\nvar user = frappe.session.user\nfrappe.call({\n    method: \"erpnext.modehero.supplier.get_type_wise_suppliers\",\n    args:{\n        supplierType:'Packaging'\n    },\n    callback: function(r) {\n        field._data = r.message;\n    }\n})\n\nfield.refresh();\n\nvar pack_material_field = frappe.web_form.get_field('packaging_material');\npack_material_field._data = [];\nvar user = frappe.session.user\nfrappe.call({\n    method: \"erpnext.modehero.package.get_packaging_material\",\n    callback: function(r) {\n        pack_material_field._data = r.message;\n    }\n})\n\n\npack_material_field.refresh();\n\nvar pack_size_field = frappe.web_form.get_field('packaging_size');\npack_size_field._data = [];\nvar user = frappe.session.user\nfrappe.call({\n    method: \"erpnext.modehero.package.get_packaging_size\",\n    callback: function(r) {\n        pack_size_field._data = r.message;\n    }\n})\n\n\npack_size_field.refresh();\n\nvar pack_color_field = frappe.web_form.get_field('color');\npack_color_field._data = [];\nvar user = frappe.session.user\nfrappe.call({\n    method: \"erpnext.modehero.fabric.get_fabric_color\",\n    callback: function(r) {\n        pack_color_field._data = r.message;\n    }\n})\n\n\npack_color_field.refresh();\n\n\nvar pack_color=frappe.web_form.get_value('color')\nif(pack_color !=\"\"){\n    frappe.call({\n        method: \"erpnext.modehero.fabric.get_color_name\",\n        args:{\n            color:pack_color\n        },\n        callback: function(r) {\n            frappe.web_form.set_value('color',r.message)\n        }\n    })\n}\n\nvar pack_size=frappe.web_form.get_value('packaging_size')\nif(pack_size !=\"\"){\n    frappe.call({\n        method: \"erpnext.modehero.package.get_size_name\",\n        args:{\n            size:pack_size\n        },\n        callback: function(r) {\n            frappe.web_form.set_value('packaging_size',r.message)\n        }\n    })\n}\n\nvar pack_material=frappe.web_form.get_value('packaging_material')\nif(pack_material !=\"\"){\n    frappe.call({\n        method: \"erpnext.modehero.package.get_material_name\",\n        args:{\n            material:pack_material\n        },\n        callback: function(r) {\n            frappe.web_form.set_value('packaging_material',r.message)\n        }\n    })\n}\n",
